# .gitignore for Drupal project managed with Composer.

# Ignore files generated from the pj template
/vendor
/web/.*
/web/*.php
/web/*.php
/web/*.txt
/web/example.gitignore
/web/web.config
/web/core
/web/libraries
/web/profiles/contrib
/web/profiles/README.txt
/web/modules/contrib
/web/modules/README.txt
/web/themes/contrib
/web/themes/README.txt
/web/sites
/web/recipes

# Ignore user generated files
/private
/tmp

# Ignore files for dev-tools etc.
/nbproject
/.vscode
/.sq
/misc
/drush/sites/demo.site.yml
/.ddev
